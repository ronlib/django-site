{% extends "polls/base.djhtml" %}
{% load staticfiles %}

{% block content %}
		{% load bootstrap3 %}
		{% bootstrap_javascript %}

		{% bootstrap_messages %}

		<form action="bla" method="post" class="form" >
				{% csrf_token %}
				{% bootstrap_form form%}
				{% bootstrap_formset formset %}
				{% buttons %}
				<button type="submit" class="btn btn-primary">
						{% bootstrap_icon "star" %} Submit
				</button>
				{% endbuttons %}
		</form>

{% endblock %}
